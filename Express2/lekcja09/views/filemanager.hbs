<div class="what">
    Filemanager<br>
    <a href="/delete">Usu≈Ñ wszystkie pliki</a>
</div>
<div class="hbs">
    <table>
        <tr class="heading">
            <th>ID</th>
            <th>obraz</th>
            <th>name</th>
            <th>type</th>
            <th>size</th>
            <th>-</th>
            <th>-</th>
            <th>-</th>
            <th>-</th>
        </tr>
        {{#each files}}
        <tr>
            <td class="id">{{id}}</td>
            <td><img class="filetype" src="/icons/{{obraz}}"></td>
            <td>{{name}}</td>
            <td>{{type}}</td>
            <td>{{size}}</td>
            <td><a href="/show/?id={{id}}">SHOW</a></td>
            <td><a href="/info/?id={{id}}">INFO</a></td>
            <td><a target="_blank" href="/download/?id={{id}}">DOWNLOAD</a></td>
            <td><a href="/delete/?id={{id}}">DELETE</a></td>
        </tr>
        {{/each}}
    </table>
</div>